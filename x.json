{
  "name": "assert",
  "description": "Assert for ChiÏ‡",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/assert.git"
  },
  "dependencies": {
    "assert": "^1.1.2"
  },
  "nodes": [
    {
      "name": "deepEqual",
      "ns": "assert",
      "async": true,
      "description": "Tests for deep equality.",
      "phrases": {
        "active": "Assert Deep Equal"
      },
      "ports": {
        "input": {
          "in": {
            "title": "Actual",
            "async": true,
            "type": "any"
          },
          "expect": {
            "title": "Expect",
            "type": "any"
          },
          "msg": {
            "title": "Message",
            "type": "string",
            "default": "Not Deeply Equal"
          }
        },
        "output": {
          "out": {
            "title": "Actual",
            "type": "any"
          }
        }
      },
      "dependencies": {
        "npm": {
          "assert": "1.x.x"
        }
      },
      "fn": "on.input.in = function() {\n  assert.deepEqual($.in, $.expect, $.msg);\n  output({ out: $.get('in') });\n}\n"
    },
    {
      "name": "equal",
      "ns": "assert",
      "async": true,
      "description": "Tests shallow, coercive equality with the equal comparison operator ( == )",
      "phrases": {
        "active": "Assert Equal"
      },
      "ports": {
        "input": {
          "in": {
            "title": "Actual",
            "async": true,
            "type": "any"
          },
          "expect": {
            "title": "Expect",
            "type": "any"
          },
          "msg": {
            "title": "Message",
            "type": "string",
            "default": "Not Equal"
          }
        },
        "output": {
          "out": {
            "title": "Actual",
            "type": "any"
          }
        }
      },
      "dependencies": {
        "npm": {
          "assert": "1.x.x"
        }
      },
      "fn": "on.input.in = function() {\n  assert.equal($.in, $.expect, $.msg);\n  output({ out: $.get('in') });\n}\n"
    },
    {
      "name": "fail",
      "ns": "assert",
      "async": true,
