{
  "name": "fail",
  "ns": "assert",
  "async": true,
  "description": "Throws an exception that displays the values for actual and expected",
  "phrases": {
    "active": "Assert Fail"
  },
  "expose": ["assert"],
  "ports": {
    "input": {
      "in": {
        "title": "Actual",
        "async": true,
        "type": "any"
      },
      "expect": {
        "title": "Expect",
        "type": "any"
      },
      "msg": {
        "title": "Message",
        "type": "string",
        "default": "Failed"
      }
    },
    "output": {
      "out": {
        "title": "Actual",
        "type": "any"
      }
    }
  }
}
